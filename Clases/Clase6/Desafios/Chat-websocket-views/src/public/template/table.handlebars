{{#if products.length}}
<table class="table containerTable">
  <thead>
    <tr>
      <th scope="col">Titulo</th>
      <th scope="col">Precio</th>
      <th scope="col">Imagen</th>
    </tr>
  </thead>
  <tbody>
        {{#each products}}
        <tr>
            <td>{{this.title}}</td>
            <td>{{this.price}}</td>
            <td><img width="60" src={{this.thumbnail}}/></td>
        </tr>
        {{/each}}
  </tbody>
</table>
<table>
{{else}}
<p>no hay productos</p>
{{/if}}